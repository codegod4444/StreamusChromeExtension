      "lodash": "npm:lodash@^3.10.1",
      "marionette": "github:marionettejs/backbone.marionette@^2.4.2",
      "mocha": "npm:mocha@^2.2.5",
      "sinon": "npm:sinon@^1.16.1"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.22",
